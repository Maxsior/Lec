# Файл для тестирования грамматики

# Объявляем переменную "e", которая будет ссылаться на объект эллипса
e:
    ellipse
        # дочерним объектом эллипса будет объект обводки
        stroke 1

r:
    rectangle
        text "some text"
        fill "blue"




arrow
    direction r e
    text "arrow"

